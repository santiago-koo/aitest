{
  "version": 1,
  "paths": {
    "/sentiment": {
      "name": "/sentiment",
      "lambdaFunction": "interpretfunction",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "create",
          "read"
        ]
      }
    },
    "/session": {
      "name": "/session",
      "lambdaFunction": "sessionfunction",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "delete"
        ],
        "guest": [
          "create"
        ]
      }
    }
  }
}
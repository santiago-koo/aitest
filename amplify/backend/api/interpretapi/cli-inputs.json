{
  "version": 1,
  "paths": {
    "/sentiment": {
      "name": "/sentiment",
      "lambdaFunction": "interpretfunction",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "create",
          "read"
        ]
      }
    },
    "/session/signin": {
      "name": "/session/signin",
      "lambdaFunction": "sessionfunction",
      "permissions": {
        "setting": "open"
      }
    },
    "/session/signup": {
      "name": "/session/signup",
      "lambdaFunction": "sessionfunction",
      "permissions": {
        "setting": "open"
      }
    },
    "/session/confirmsignup": {
      "name": "/session/confirmsignup",
      "lambdaFunction": "sessionfunction",
      "permissions": {
        "setting": "open"
      }
    }
  }
}